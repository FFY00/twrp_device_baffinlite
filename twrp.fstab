/system       ext4  /dev/block/mmcblk0p19
/cache        ext4  /dev/block/mmcblk0p18
/data         ext4  /dev/block/mmcblk0p21
/preload      ext4  /dev/block/mmcblk0p20  flags=display="Preload"
/efs          ext4  /dev/block/mmcblk0p17  flags=display="EFS";backup=1
/boot         emmc  /dev/block/mmcblk0p5
/recovery     emmc  /dev/block/mmcblk0p6
/external_sd  vfat  /dev/block/mmcblk1p1   flags=display="Micro SD";storage;wipeingui;removable
